@article{AVGUSTINOVICH2001444,
title = {On the metrical rigidity of binary codes},
journal = {Electronic Notes in Discrete Mathematics},
volume = {6},
pages = {444-451},
year = {2001},
note = {WCC2001, International Workshop on Coding and Cryptography},
issn = {1571-0653},
doi = {https://doi.org/10.1016/S1571-0653(04)00196-9},
url = {https://www.sciencedirect.com/science/article/pii/S1571065304001969},
author = {S.V. Avgustinovich and F.I. Solov'eva},
keywords = {Metrically Rigid Codes, Strongly Rigid Codes, 2-(, λ)-design, Uniformly Packed Codes},
abstract = {A code C in the n-dimensional vector space En over GF(2) is called metrically rigid if every isometry I: C → En with respect to the Hamming metric is extendable to an isometry of the whole space En. A code C is reduced if it contains the all-zero vector. For n large enough the metrical rigidity of length n reduced binary codes containing a 2-(n,k,λ)-design is proved. The class of such codes includes all the families of uniformly packed codes of sufficiently large length satisfying the condition d-ρ ≥ 2, where d is the code distance and ρ is the covering radius.}
}


